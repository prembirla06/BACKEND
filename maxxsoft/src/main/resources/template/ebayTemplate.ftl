<body class="catalog-product-view">
<div class="wrapper">
  <div class="page">
    <div class="header-language-background">
      <div class="header-language-container">
        <div class="store-language-container"> </div>
        <p class="welcome-msg">Günstige Möbel online kaufen</p>
      </div>
    </div>
    <header id="header" class="page-header">
      <div class="page-header-container"><a class="logo"> <img src="https://www.moebel-guenstig24.de/media/logo/stores/1/moebel-guenstig24_logo.png" target="_blank" alt="Günstige Möbel online kaufen - Möbel günstig kaufen im Onlineshop von Moebel-guenstig24" class="large" /> <img src="https://www.moebel-guenstig24.de/media/logo/stores/1/moebel-guenstig24_logo.png" alt="Günstige Möbel online kaufen - Möbel günstig kaufen im Onlineshop von Moebel-guenstig24" class="small" /> </a>
        <div class="store-language-container"></div>
        
        <!-- Skip Links -->
        
        <div class="skip-links"> <a href="#header-nav" class="skip-link skip-nav"> <span class="icon"></span> <span class="label">Menü</span> </a> <a href="#header-search" class="skip-link skip-search"> <span class="icon"></span> <span class="label">Suche</span> </a>
          
        </div>
        
        <!-- Account -->
        
        
      </div>
    </header>
    <div class="main-container col1-layout">
      <div class="main">
        <div class="breadcrumbs">
          <ul>
            <li class="product"> <strong>${name}</strong></li>
          </ul>
        </div>
        <div class="col-main"> 
          <div id="messages_product_view"></div>
          <div class="product-view">
            <div class="product-essential">
                <div class="product-img-box">
                  <div class="product-name">
                    <h1>${name}</h1>
                  </div>			  
				  
                  <div class="product-image product-image-zoom">
                    <div class="product-image-gallery"> <a href="${hauptbild}" target="_blank"><img id="image-main"
             itemprop="image"
             class="gallery-image visible"
             src="${hauptbild}"
             alt="${alttitlebild}"
             title="${alttitlebild}" /> </a>
			  </div>
                  </div>

                </div>
                <div class="product-shop">
                  <div class="product-name"> <span class="h1" itemprop="name">${name}</span> </div>
                  
                  <div class="extra-info"> 
                    
                    <!-- Added by TM, Lagerampel --> 
                    
                    Lagerstatus:
                    <div class="stockindicator stockindicator-${color_stock}"></div>
                    <!-- END Added by TM, Lagerampel --> 
                    
                    <!-- Delivery timer insert by TM -->
                    
                    <p class="delivery-time"> Lieferzeit: ${delivery_time} </p>
                    <!-- Deleted by TM
                                <p class="availability in-stock">
            <span class="label">Verfügbarkeit:</span>
            <span class="value">Auf Lager</span>
        </p>
                        END Deleted by TM --> 
                  </div>
                  <div class="short-description">
                    <div class="std" itemprop="description">
                      ${short_description}
                    </div>
                  </div>
                </div>

                <div class="clearer"></div>
              
            </div>
            <div class="product-collateral toggle-content tabs">
                  <div class="tab-content">
                    <h2>BESCHREIBUNG</h2>
                    <div class="std">
                      ${description}
                    </div>
                  </div>
            </div>
			
			

			
			
			<#list pictureList as picture>
			<div class="product-image product-image-zoom">
            <div class="product-image-gallery"> <a href="${picture}"  target="_blank"><img style="max-width:100%;" id="image-main"
             itemprop="image"
             class="gallery-image visible"
             src="${picture}"/> </a>
			  </div>
			  </div>
			</#list>
			
			
    
          </div>
        </div>
      </div>
    </div>
    <div class="footer-container">
      <div class="footer">
        <address class="copyright">
        &copy; 2015 moebel-guenstig24.de / TecMaXX GmbH
        </address>
      </div>
    </div>
  </div>
